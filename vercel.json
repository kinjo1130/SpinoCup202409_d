{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/react/play/(.*)", "dest": "/react/play/$1" },
    { "src": "/vue/score/(.*)", "dest": "/vue/score/$1" },
    {
      "src": "/svelte/selecting-cars/(.*)",
      "dest": "/svelte/selecting-cars/$1"
    },
    { "src": "/solidjs/title/(.*)", "dest": "/solidjs/title/$1" },
    { "src": "/vanilla/(.*)", "dest": "/vanilla/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
